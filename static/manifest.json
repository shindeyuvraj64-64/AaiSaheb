{
  "name": "aai Saheb - Women's Safety & Empowerment Platform",
  "short_name": "aai Saheb",
  "description": "Comprehensive women's safety and empowerment platform for Maharashtra with emergency SOS, education, employment, and community support",
  "version": "1.0.0",
  "start_url": "/",
  "display": "standalone",
  "orientation": "portrait-primary",
  "theme_color": "#3F51B5",
  "background_color": "#FAFAFA",
  "lang": "en",
  "scope": "/",
  "categories": ["safety", "social", "education", "productivity"],
  "icons": [
    {
      "src": "data:image/svg+xml;base64,",
      "sizes": "192x192",
      "type": "image/svg+xml",
      "purpose": "any maskable"
    },
    {
      "src": "data:image/svg+xml;base64,",
      "sizes": "512x512",
      "type": "image/svg+xml",
      "purpose": "any maskable"
    }
  ],
  "screenshots": [
    {
      "src": "data:image/svg+xml;base64,",
      "sizes": "360x780",
      "type": "image/svg+xml",
      "form_factor": "narrow"
    }
  ],
  "shortcuts": [
    {
      "name": "Emergency SOS",
      "description": "Quick access to emergency services",
      "url": "/sos",
      "icons": [
        {
          "src": "data:image/svg+xml;base64,",
          "sizes": "96x96",
          "type": "image/svg+xml"
        }
      ]
    },
    {
      "name": "Safety Resources",
      "description": "Find nearby police stations and hospitals",
      "url": "/safety_resources",
      "icons": [
        {
          "src": "data:image/svg+xml;base64,",
          "sizes": "96x96",
          "type": "image/svg+xml"
        }
      ]
    },
    {
      "name": "Community",
      "description": "Connect with support community",
      "url": "/community",
      "icons": [
        {
          "src": "data:image/svg+xml;base64,",
          "sizes": "96x96",
          "type": "image/svg+xml"
        }
      ]
    },
    {
      "name": "Education",
      "description": "Access learning modules",
      "url": "/education",
      "icons": [
        {
          "src": "data:image/svg+xml;base64,",
          "sizes": "96x96",
          "type": "image/svg+xml"
        }
      ]
    }
  ],
  "share_target": {
    "action": "/share",
    "method": "POST",
    "enctype": "multipart/form-data",
    "params": {
      "title": "title",
      "text": "text",
      "url": "url",
      "files": [
        {
          "name": "evidence",
          "accept": ["image/*", "audio/*", "video/*"]
        }
      ]
    }
  },
  "prefer_related_applications": false,
  "related_applications": [],
  "edge_side_panel": {
    "preferred_width": 400
  },
  "file_handlers": [
    {
      "action": "/handle-file",
      "accept": {
        "image/*": [".jpg", ".jpeg", ".png", ".gif", ".webp"],
        "audio/*": [".mp3", ".wav", ".ogg", ".m4a"],
        "video/*": [".mp4", ".webm", ".mov"]
      }
    }
  ],
  "protocol_handlers": [
    {
      "protocol": "tel",
      "url": "/call?number=%s"
    }
  ],
  "launch_handler": {
    "client_mode": "focus-existing"
  },
  "display_override": ["window-controls-overlay", "standalone", "minimal-ui"],
  "handle_links": "preferred",
  "id": "aai-saheb-app",
  "dir": "ltr",
  "iarc_rating_id": "",
  "features": [
    "geolocation",
    "camera",
    "microphone",
    "push-notifications",
    "offline-support",
    "background-sync"
  ]
}
